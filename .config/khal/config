[calendars]

[[local]]
path = ~/.local/calendars/enabled/*
type = discover

[locale]
local_timezone = Europe/Brussels
default_timezone = Europe/Brussels
timeformat = %H:%M
dateformat = %d-%b-%Y
longdateformat = %d-%b-%Y
datetimeformat = %d-%b-%Y %H:%M
longdatetimeformat = %d-%b-%Y %H:%M

[default]
default_calendar = user1

[view]
event_format = {calendar-color}{cancelled}{start}-{end} {title}{repeat-symbol}{description-separator}{location}-{description}{reset}
